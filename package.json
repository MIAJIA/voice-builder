{
  "name": "voice-builder",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "test": "vitest",
    "test:run": "vitest run",
    "agent:social-listening": "npx tsx src/agents/social-listening/cli.ts",
    "agent:social-listening:setup": "npx tsx src/agents/social-listening/cli.ts setup",
    "agent:social-listening:once": "npx tsx src/agents/social-listening/cli.ts once",
    "agent:social-listening:start": "npx tsx src/agents/social-listening/cli.ts start",
    "agent:content-generator": "node --env-file=.env.local --import tsx src/agents/content-generator/cli.ts",
    "agent:content-generator:setup": "node --env-file=.env.local --import tsx src/agents/content-generator/cli.ts setup",
    "agent:content-generator:generate": "node --env-file=.env.local --import tsx src/agents/content-generator/cli.ts generate",
    "agent:content-generator:ideas": "node --env-file=.env.local --import tsx src/agents/content-generator/cli.ts ideas"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.72.1",
    "@google/generative-ai": "^0.24.1",
    "@microsoft/fetch-event-source": "^2.0.1",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "html2canvas": "^1.4.1",
    "lucide-react": "^0.563.0",
    "next": "16.1.6",
    "openai": "^6.17.0",
    "posthog-js": "^1.336.4",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "tailwind-merge": "^3.4.0",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.1.2",
    "babel-plugin-react-compiler": "1.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "jsdom": "^27.4.0",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5",
    "vitest": "^4.0.18"
  }
}
